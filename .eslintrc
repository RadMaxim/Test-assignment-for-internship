{
    "env": {
      "browser": true,
      "es2021": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 12,
      "sourceType": "module"
    },
    "rules": {
      "indent": ["error", 2],
      "quotes": ["error", "single"],
      "semi": ["error", "always"],
      "no-unused-vars": ["warn"],
      "no-console": "off",
      "curly": ["error", "all"],
      "prefer-const": "error",
      "no-trailing-spaces": "error",
      "eqeqeq": ["error", "always"]
    }
  }
  